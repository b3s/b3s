{
  "name": "app",
  "dependencies": {
    "@dnd-kit/core": "^6.0.6",
    "@dnd-kit/sortable": "^7.0.1",
    "@fortawesome/fontawesome-free": "^6.2.1",
    "@rails/ujs": "^7.0.3-1",
    "autoprefixer": "^10.4.13",
    "downshift": "^7.1.2",
    "dropzone": "^6.0.0-beta.2",
    "esbuild": "^0.16.17",
    "esbuild-plugin-import-glob": "^0.1.1",
    "esbuild-rails": "^1.0.3",
    "jquery": "^3.6.3",
    "match-sorter": "^6.3.1",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "postcss-import-ext-glob": "^2.1.1",
    "postcss-mixins": "^9.0.4",
    "postcss-nested": "^6.0.0",
    "postcss-url": "^10.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react_ujs": "^2.6.2",
    "underscore": "^1.13.6",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/dropzone": "^5.7.4",
    "@types/jquery": "^3.5.16",
    "@types/rails__ujs": "^6.0.1",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.2.7",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.4"
  },
  "scripts": {
    "lint": "eslint 'app/javascript'",
    "build": "run build-dev --minify",
    "build-dev": "esbuild app/javascript/application.ts app/javascript/mobile.ts --bundle --sourcemap --outdir=app/assets/builds --target=es2020,chrome64,edge79,firefox62,safari11.1",
    "build:css": "postcss ./app/assets/stylesheets/**/*.postcss --base ./app/assets/stylesheets --dir ./app/assets/builds --ext css",
    "prettier": "prettier app/javascript app/assets/stylesheets --check",
    "prettier:fix": "prettier app/javascript app/assets/stylesheets --write"
  },
  "packageManager": "yarn@3.2.3"
}
