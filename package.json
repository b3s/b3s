{
  "name": "app",
  "dependencies": {
    "@dnd-kit/core": "^6.0.8",
    "@dnd-kit/sortable": "^7.0.2",
    "@fortawesome/fontawesome-free": "^6.2.1",
    "@rails/ujs": "^7.0.6",
    "autoprefixer": "^10.4.15",
    "downshift": "^8.1.0",
    "dropzone": "^6.0.0-beta.2",
    "esbuild": "^0.19.2",
    "jquery": "^3.7.0",
    "match-sorter": "^6.3.1",
    "postcss": "^8.4.31",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "postcss-import-ext-glob": "^2.1.1",
    "postcss-mixins": "^9.0.4",
    "postcss-nested": "^6.0.1",
    "postcss-url": "^10.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react_ujs": "^3.1.1",
    "underscore": "^1.13.6",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/dropzone": "^5.7.4",
    "@types/jquery": "^3.5.16",
    "@types/rails__ujs": "^6.0.1",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.0.2",
    "typescript": "^5.1.6"
  },
  "scripts": {
    "lint": "eslint 'app/javascript'",
    "build": "run build-dev --minify",
    "build-dev": "esbuild app/javascript/application.ts app/javascript/mobile.ts --bundle --sourcemap --outdir=app/assets/builds --target=es2020,chrome64,edge79,firefox62,safari11.1",
    "build:css": "postcss ./app/assets/stylesheets/**/*.postcss --base ./app/assets/stylesheets --dir ./app/assets/builds --ext css",
    "prettier": "prettier app/javascript app/assets/stylesheets --check",
    "prettier:fix": "prettier app/javascript app/assets/stylesheets --write"
  },
  "packageManager": "yarn@3.2.3"
}
